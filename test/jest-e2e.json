{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@rms/file-loader/(.*)": "<rootDir>/../libs/file-loader/src/$1",
    "@rms/file-loader": "<rootDir>/../libs/file-loader/src",
    "@rms/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@rms/config": "<rootDir>/../libs/config/src",
    "@rms/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@rms/database": "<rootDir>/../libs/database/src"
  }
}