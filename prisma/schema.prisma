// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  title        String  @id
  url          String
  isPrivate      Boolean @default(false)
  allowedUsers User[]
}

model User {
  login         String  @id
  privateVideos Video[]
}
